{
    "name": "uzulla/enhanced-redis-session-handler",
    "description": "PHPのセッション管理をRedis/ValKeyで実装する拡張可能なセッションハンドラライブラリ",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "uzulla / Junichi Ishida",
            "email": "zishida@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-redis": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Uzulla\\EnhancedRedisSessionHandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uzulla\\EnhancedRedisSessionHandler\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
